<div class="my-homes">
	<div class="container">
		<div class="row">
			<div class="jumbo-wrapper col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 col-xs-12">
				<h1>My Homes</h1>
				<h2>All the homes (and neighborhoods) you've checked out so far.</h2>
				<ul class="list-group">
				{{#each}}
				  {{#link-to 'property' this}}
				    <li class="property list-group-item row">
				    	<p>{{address}}</p>
				    	<h3>${{price}}</h3>
				    	<div>
				    		<div class="col-md-4 col-sm-4 col-xs-6 home-info">
				    			<h4>Bedrooms: {{bedrooms}}</h4>
				    		</div>
				    		<div class="col-md-4 col-sm-4 col-xs-6 home-info">
							  	<h4>Bathrooms: {{bathrooms}}</h4>
						  	</div>
				    	</div>
				    </li>
				  {{/link-to}}
				{{/each}}
				</ul>
			</div>
		</div>
	</div>
</div>

